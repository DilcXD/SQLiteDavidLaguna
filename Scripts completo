CREATE TABLE Carrera(idCarrera INTEGER PRIMARY KEY AUTOINCREMENT,
			Nombre varchar (30),
			Duracion INTEGER);
			
CREATE TABLE Alumno (idAlumno INTEGER PRIMARY KEY AUTOINCREMENT,
    		id_Carrera INTEGER,
		    Nombre varchar(30),
    		ApPaterno varchar(30),
    		ApMaterno varchar(30),
    		Email varchar(40),
    		Edad INTEGER,
    		Sexo TEXT(1),
    		FOREIGN KEY(id_Carrera) REFERENCES Carrera(idCarrera));

INSERT INTO Carrera(Nombre,Duracion) VALUES ('Ingenieria Computacion',5)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Ingenieria Industrial',5)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Ingenieria Sistemas',5)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Ingenieria Biotecnologia',4)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Ingenieria Civil',4)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Ingenieria Telecomunicaciones',4)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Licenciatura en Finanzas',3)
INSERT INTO Carrera(Nombre,Duracion) VALUES ('Licenciatura en Psicologia',3)

SELECT * FROM Carrera

UPDATE Carrera SET Nombre= 'Ingenieria en Sistemas Computacionales', Duracion=3 WHERE idCarrera=1

DELETE FROM Carrera WHERE idCarrera = 3;

SELECT * FROM Carrera WHERE idCarrera=5
SELECT * FROM Carrera WHERE Nombre= 'Ingenieria Civil'
SELECT * FROM Carrera WHERE Duracion=4

UPDATE Carrera SET Nombre='Licenciatura en Economia' WHERE idCarrera=6

SELECT * FROM Carrera WHERE Duracion <> 4
SELECT * FROM Carrera WHERE Duracion > 3
SELECT * FROM Carrera WHERE Duracion < 5
SELECT * FROM Carrera WHERE Duracion >= 4
SELECT * FROM Carrera WHERE Duracion <= 4

