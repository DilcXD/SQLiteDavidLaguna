AND -- y
SELECT * FROM Alumno WHERE id_Carrera=2 AND Sexo='F'

OR -- Y/o
SELECT * FROM Alumno WHERE id_Carrera=2 OR Sexo='H'

NOT -- Negacion
SELECT * FROM Alumno WHERE NOT Sexo='F'

-- Operador Between

SELECT * FROM Carrera WHERE Duracion BETWEEN 3 AND 4
SELECT * FROM Carrera WHERE NOT Duracion BETWEEN 4 AND 5

SELECT * FROM Alumno WHERE Edad BETWEEN 21 AND 23

-- Operador IN

SELECT * FROM Carrera WHERE Nombre IN('Ingenieria Industrial','Ingenieria Biotecnologia','Ingenieria Civil')

SELECT * FROM Carrera WHERE idCarrera IN(2,5,7,8)

SELECT * FROM Carrera WHERE NOT Nombre IN('Ingenieria Industrial','Ingenieria Biotecnologia','Ingenieria Civil')

-- Operador LIKE
SELECT * FROM Carrera WHERE Nombre LIKE 'L%'
SELECT * FROM Carrera WHERE Nombre LIKE '%a'
SELECT * FROM Carrera WHERE Nombre LIKE '%tria%'

SELECT * FROM Carrera WHERE Nombre NOT LIKE 'I%'
